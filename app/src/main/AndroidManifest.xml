<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dj.agent">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_g"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_g"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <meta-data

            android:name="com.google.android.gms.version"

            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.gms.ads.com.dj.agent"
            android:value="@string/admob_app_id"
            >
        </meta-data>
        <meta-data android:name = "com.google.android.gms.ads.AD_MANAGER_APP"
                android:value = "true">
        </meta-data>
        <activity android:name=".MainActivity">

        </activity>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
        <activity android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".finishday">
        </activity>
        <activity android:name=".insert">
        </activity>
        <activity android:name=".main">
        </activity>
        <activity android:name=".startday">
        </activity>
        <activity android:name=".infor1">
        </activity>
        <activity android:name=".infor2">
        </activity>
        <activity android:name=".infor3">
        </activity>
        <activity android:name=".infor4">
        </activity>

    </application>

</manifest>